<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.parcial2.DecimoController">
   <children>
      <Label fx:id="lblTitulo" text="Agregar Cliente">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <GridPane>
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Nombre" />
            <Label text="Apellido" GridPane.rowIndex="1" />
            <Label text="Cédula" GridPane.rowIndex="2" />
            <Label text="Teléfono" GridPane.rowIndex="3" />
            <Label text="Email" GridPane.rowIndex="4" />
            <Label text="Representante" GridPane.rowIndex="5" />
            <TextField fx:id="txtNombre" GridPane.columnIndex="1" />
            <TextField fx:id="txtApellido" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="txtCedula" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="txtTelefono" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="txtEmail" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField fx:id="txtRepresentante" GridPane.columnIndex="1" GridPane.rowIndex="5" />
         </children>
      </GridPane>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="guardar" mnemonicParsing="false" onAction="#guardado" text="Guardar">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="cancelar" mnemonicParsing="false" onAction="#cancelacion" text="Cancelar" />
         </children>
      </HBox>
   </children>
